<h1>Sign In to Rojo</h1>

<div>
  <%= form_tag create_session_path do %>

<div>    <%= label_tag :email, "Enter your email address" %>
    <%= email_field_tag :email %></div>

<div>    <%= label_tag :password, "Enter your password" %>
    <%= password_field_tag :password %></div>

    <%= submit_tag 'Sign In' %>

  <% end %>
</div>
